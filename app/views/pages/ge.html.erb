      } 
      
      function fly_to(to){ 
        var la = ge.createLookAt(''); 
              
        switch(to){ 
          case 'tra': 
          la.set(35.218404, -97.485436, 0, ge.ALTITUDE_RELATIVE_TO_GROUND, 0, 0, 10000); 
          break; 
          case 'cur': 
          la.set(35.206167, -97.477376, 0, ge.ALTITUDE_RELATIVE_TO_GROUND, 0, 0, 600); 
          break; 
          case 'lin': 
          la.set(35.202441, -97.469126, 0, ge.ALTITUDE_RELATIVE_TO_GROUND, 0, 0, 600); 
          break; 
          case 'quo': 
          la.set(35.231081, -97.482285, 0, ge.ALTITUDE_RELATIVE_TO_GROUND, 0, 0, 600); 
          break; 
          case 'dil': 
          la.set(35.240502, -97.48641, 0, ge.ALTITUDE_RELATIVE_TO_GROUND, 0, 0, 600); 
          break; 
          case 'rep': 
          la.set(35.232282, -97.493845, 0, ge.ALTITUDE_RELATIVE_TO_GROUND, 0, 0, 600); 
          break; 
        } 
                  
        ge.getView().setAbstractView(la); 
            
        } 
          
      function getInheritedVisibility(layer) { 
        if (!layer.getVisibility()) { 
          return false; 
        } else { 
          var parent = layer.getParentNode(); 
          if (!parent) 
          return true; 
          
          return getInheritedVisibility(parent); 
        } 
      } 
      
      google.setOnLoadCallback(init); 
        
