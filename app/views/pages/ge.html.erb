      function getNL(kmlURL){
  		  var nl = ge.createNetworkLink("");
  		  var link = ge.createLink("");
  		  link.setHref(kmlURL);
  		  nl.setLink(link);
  		  ge.getGlobe().getFeatures().appendChild(nl);
  		  return nl;
      }
        
      // Options //
    
      function updateOptions() {
    		var options = ge.getOptions();
    		var form = document.options;
        
    		kmzCities.setVisibility(document.options.cities.checked);
    		kmzFloodplains.setVisibility(document.options.floodplains.checked);
    		kmzSections.setVisibility(document.options.sections.checked);
          
    		options.setScaleLegendVisibility(form.scale.checked);
          
        if (form.borders.checked) {
    			ge.getLayerRoot().enableLayerById(ge.LAYER_BORDERS, true);
        } else {
    			ge.getLayerRoot().enableLayerById(ge.LAYER_BORDERS, false);
        }
        
        if (form.roads.checked) {
    			ge.getLayerRoot().enableLayerById(ge.LAYER_ROADS, true);
        } else {
    			ge.getLayerRoot().enableLayerById(ge.LAYER_ROADS, false);
        }
        
      }
      
